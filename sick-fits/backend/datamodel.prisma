type User {
  id: ID! @unique
  name: String!
  email: String!  
}

type Item {
  id: ID! @unique
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!
  #DateTime is prisma, not GraphQL
  createdAt: DateTime!
  updatedAt: DateTime!
  #user: User!
}
